{
  "name": "temporal-patterns-typescript",
  "version": "0.3.30",
  "description": "Temporal pattern examples",
  "main": "index.js",
  "scripts": {
    "_copy_env_example": "cp -n env.example .env || true",
    "_docker:build:up": "docker compose watch",
    "docker:up": "npm run _copy_env_example && npm run _docker:build:up",
    "docker:down": "docker compose down",
    "docker:reset": "docker compose down -v && npm run docker:up",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts",
    "start:temporal": "ts-node-dev --respawn --transpile-only examples/temporal/worker.ts",
    "start:meshflow": "ts-node-dev --respawn --transpile-only examples/meshflow/worker.ts",
    "start:server": "ts-node-dev --respawn --transpile-only server/index.ts",
    "start:all": "npm-run-all -p start:server start:temporal start:meshflow"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "@honeycombio/opentelemetry-node": "^0.7.2",
    "@hotmeshio/hotmesh": "0.3.30",
    "@opentelemetry/auto-instrumentations-node": "^0.47.1",
    "@opentelemetry/sdk-node": "^0.52.1",
    "@temporalio/client": "^1.10.3",
    "@temporalio/worker": "^1.10.3",
    "@temporalio/workflow": "^1.10.3",
    "express": "^4.19.2",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.13.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0"
}
